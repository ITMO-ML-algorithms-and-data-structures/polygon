#include <iostream>
#include <vector>

int main() {
using namespace std;
    vector <int> arr1 = {1,1,2,3};
    bool outp = true;

    for (int i = 0; i < arr1.size()-1; i++) {
        if (arr1[i] > arr1[i + 1]){
            outp = false;
            break;          
        }
    }
    cout << outp;

    return 0;
}
