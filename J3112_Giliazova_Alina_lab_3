#include <iostream>
#include <string>
#include <fstream>

using namespace std;

int main() {
    string input;

    ifstream inputFile("dataset4.txt");
    getline(inputFile, input);
    inputFile.close();

    string result;
    char currentChar = '\0';
    int count = 0;

    // Основной цикл по строке - O(n)
    for (char ch : input) {
        if (ch != ' ') {  // O(1)
            if (ch == currentChar) {
                count++;  // O(1)
            } else {
                if (count > 0) {
                    result += currentChar; // O(1)
                    result += to_string(count);  // O(log(count))
                }
                currentChar = ch;  // O(1)
                count = 1;  // O(1)
            }
        }
    }

    // Обрабатываем последний символ цикла - O(1)
    if (count > 0) {
        result += currentChar;       // O(1)
        result += to_string(count);  // O(log(count))
    }

    cout << "Результат: " << result << endl;
