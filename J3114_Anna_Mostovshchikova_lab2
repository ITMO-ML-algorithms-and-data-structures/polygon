#include <iostream>
#include <set>

using namespace std;

int main() {

    short size; 
    cout << "enter the size of array such that (1 <= size <= 10000): " << endl;
    cin >> size;


    const short max_size = 10000; 
    if (size > max_size) {
        cout << "the size of array must not be greater than 10000" << endl;
        return 1;
    }
    if (size < 1) {
        cout << "the size of array must not be less than 1" << endl;
        return 1;
    }


    short arr[max_size];  
    cout << "enter the elements of array such that (1 <= element <= 210): " << endl;
    for (short i = 0; i < size; i++) {
        cin >> arr[i];
        if (arr[i] > 210) {
            cout << "the element of array must not be greater than 210" << endl;
            return 1;
        }
        else if (arr[i] < 1) {
            cout << "the element of array must not be less than 1" << endl;
            return 1;
        }
    }

    set<short> unique_elements; 
    short result[max_size]; 
    short result_size = 0; 

    for (short i = 0; i < size; i++) {
        if (unique_elements.insert(arr[i]).second) { 
            result[result_size++] = arr[i];
        }
    }


    cout << "your array without duplicates: ";
    for (short i = 0; i < result_size; i++) {
        cout << result[i] << " ";
    }

    cout << endl;

    return 0;
}

//память = 30 байт + 2 * (2 байт * max_size) + (2 байт * size)
