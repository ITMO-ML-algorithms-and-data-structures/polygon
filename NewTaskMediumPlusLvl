#include <iostream>
#include <vector>
using namespace std;

int main() {
    vector <int> arr1 = {1,1,2,3, };
    bool outp = true;

    for (int i = 0; i < arr1.size()-1; i++) {
        if (arr1[i] <= arr1[i + 1]){
            outp =true;   
                }       
        else {
            outp = false;
            break;
        }
    }

    cout << outp;

    

    return 0;
}
