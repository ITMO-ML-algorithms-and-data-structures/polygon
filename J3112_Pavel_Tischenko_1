// вариант 1 hard- "найти сумму всех чисел массива, которые кратны 3"
#include <iostream>
#include <vector>
using namespace std;

int main()
{
    int m = 0;
    vector<int> lst = {-123, 23, 44, 66, 122, 0, -34, 33, 45};

    for (int i = 0; i < lst.size(); i++)
    {
        if (lst[i] % 3 == 0)
        {
            m += lst[i];
        }
    }
    cout << m;
    return 0;
}
