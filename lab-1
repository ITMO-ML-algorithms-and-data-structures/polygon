#include <iostream>

int main()
{
    int arr[5] {2,4,5,8,11};
    for (int i = 0; i < 5; i++){
        for (int j = 2; j <= arr[i]/2; j++){
            if (arr [i] % j == 0 && arr[i] != j){
                arr [i] = 0;
            }
        }
        if (arr [i] != 0){
             std::cout<<arr[i] << " ";
        }
    }
    return 0;
}
