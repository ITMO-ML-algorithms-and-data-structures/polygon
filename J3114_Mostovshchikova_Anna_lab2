#include <iostream>
#include <set>

using namespace std;

int main() {

    int size;
    cout << "enter the size of array such that (size < 10000): " << endl;
    cin >> size;

    const int max_size = 10000;
    if (size > max_size) {
        cout << "the size of array must not exceed 10000" << endl;
        return 1;
    }

    int arr[max_size];
    cout << "enter the elements of array such that (0 < element < 210): " << endl;
    for (int i = 0; i < size; i++) {
        cin >> arr[i];
    }

    set<int> unique_elements;
    int result[max_size];
    int result_size = 0;

    for (int i = 0; i < size; i++) {
        if (unique_elements.insert(arr[i]).second) {
            result[result_size++] = arr[i];
        }
    }

    cout << "your array without duplicates: ";
    for (int i = 0; i < result_size; i++) {
        cout << result[i] << " ";
    }

    cout << endl;

    return 0;
}
