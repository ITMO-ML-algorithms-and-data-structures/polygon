


#include <iostream>

using namespace std;

int main() {

    int num[10] = {5, -3, 81, -7, 19, 47, -38, 99, 105, -6};

    int minvalue = 1000000; 

    for (int index = 0; index < 10; index++) {
        if (num[index] < minvalue) {
            minvalue = num[index];
        }
    }

    cout << minvalue << endl;

    return 0;
}
