#include <iostream>
#include <vector>

int main()
{
    using namespace std;

    //Найти произведение всех чисел в массиве.

    int rows, columns;
    int product = 1;
    
    cout << "Number of rows: ";
    cin >> rows;
    cout << "Number of columns: ";
    cin >> columns;

    vector<vector<int>> matriz(rows, vector<int>(columns));

    cout << "Enter elements:" << endl;
    for (int i = 0; i < rows; ++i) {
        for (int j = 0; j < columns; ++j) {
            cout << "Element [" << i << "][" << j << "]: ";
            cin >> matriz[i][j];
        }
    }

    for (int i = 0; i < rows; ++i) {
        for (int j = 0; j < columns; ++j) {
            product *= matriz[i][j];
        }
    }

    cout << "Product of all elements: " << product << endl;
    return 0;
} 
