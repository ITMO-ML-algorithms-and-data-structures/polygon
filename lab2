#include <bits/stdc++.h>
using namespace std;

bool areIsomorphic(string str1, string str2)
{
    unordered_map<char, int> iso;
 
    int size1 = str1.size();
    int size2 = str2.size();
 
    if (size1 != size2)
        return false;
 
    for (int i = 0; i < size1; i++) {
        
        if (iso.find(str1[i]) == iso.end()) {
            iso[str1[i]] = str1[i] - str2[i];
        }
 
        else if (iso[str1[i]]
                 != (str1[i] - str2[i])) {
            return false;
        }
    }
    return true;
}

int main()
{
    string s1 = "egg";
    string s2 = "add";

    bool ans = areIsomorphic(s1, s2);
    if (ans)
        cout << "True";
    else
        cout << "False";
    return 0;
}
