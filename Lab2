#include <iostream>
#include <map>
#include <fstream>
using namespace std;

int main(int argc, char *argv[])
{
	//ifstream file("ttt.txt");
	//string line[50];
	//file.getline(line,50);
	int n = 4;
	string array1[] = {"arbuz","dub","667","arbuz"};
	
	std::map<std::string,int> dict1;
	
	for(int i = 0;i<=n-1;i++){dict1[array1[i]]=0;};
	
	for(int i = 0;i<=n-1;i++){dict1[array1[i]]+=1;};
	
	for(int i = 0;i<=n-1;i++){cout<<dict1[array1[i]];}
}
